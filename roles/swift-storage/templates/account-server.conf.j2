[DEFAULT]
devices = /srv/1/node
mount_check = false
disable_fallocate = true
bind_ip = {{ hostvars[ansible_hostname]['ansible_' + iface].ipv4.address }}
workers = 2
user = swift

[pipeline:main]
pipeline = account-server

[app:account-server]
use = egg:swift#account

[account-replicator]

[account-auditor]

[account-reaper]
